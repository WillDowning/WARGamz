public class Hero extends Character
{
  private int x;
  private int y;
  private int hp;
  private int ammo;
  
  public Hero(int newX, int newY, String newName, int newHp, int newAmmo){
      super( newX, newY, newName, newHp, newAmmo);
      
  }
  public int getX(){
    return x;
    }
  public int getY(){
    return y;
    }
    
}
